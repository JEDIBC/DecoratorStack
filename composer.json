{
  "name": "JEDIBC/DecoratorStack",
  "description": "A simple tool for stacking decorators",
  "keywords": [
    "decorator",
    "stack"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Bruno CHALOPIN",
      "homepage": "https://github.com/JEDIBC"
    }
  ],
  "require": {
    "php": ">=5.4",
    "psr/log": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "m6web/Symfony2-coding-standard": "~1.1",
    "m6web/coke": "~1.0"
  },
  "autoload": {
    "psr-4": {
      "DecoratorStack\\": "src/DecoratorStack/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "bin-dir": "bin"
  }
}